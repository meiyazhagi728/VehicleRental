{
  "name": "vehicle-rental-system",
  "version": "1.0.0",
  "description": "MERN Stack Vehicle Rental System with GPS integration",
  "main": "server.js",
  "scripts": {
    "start": "node start-server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev-full": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "setup": "node setup-env.js",
    "test-db": "node test-db-connection.js",
    "seed-mechanics": "node run-seed-mechanics.js"
  },
  "keywords": [
    "vehicle",
    "rental",
    "mern",
    "gps",
    "booking"
  ],
  "author": "Vehicle Rental Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.8",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "stripe": "^19.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
